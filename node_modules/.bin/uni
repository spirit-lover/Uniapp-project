#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/软件专业/Uniapp/easyjob-app/node_modules/.store/@dcloudio+vite-plugin-uni@3.0.0-3090420231025001/node_modules/@dcloudio"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/软件专业/Uniapp/easyjob-app/node_modules/.store/@dcloudio+vite-plugin-uni@3.0.0-3090420231025001/node_modules/@dcloudio"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/@dcloudio+vite-plugin-uni@3.0.0-3090420231025001/node_modules/@dcloudio/vite-plugin-uni/bin/uni.js" "$@"
else
  exec node  "$basedir/../.store/@dcloudio+vite-plugin-uni@3.0.0-3090420231025001/node_modules/@dcloudio/vite-plugin-uni/bin/uni.js" "$@"
fi
