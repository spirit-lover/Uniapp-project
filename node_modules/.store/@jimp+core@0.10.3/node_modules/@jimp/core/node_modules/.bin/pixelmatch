#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/软件专业/Uniapp/easyjob-app/node_modules/.store/pixelmatch@4.0.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/软件专业/Uniapp/easyjob-app/node_modules/.store/pixelmatch@4.0.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../pixelmatch@4.0.2/node_modules/pixelmatch/bin/pixelmatch" "$@"
else
  exec node  "$basedir/../../../../../../pixelmatch@4.0.2/node_modules/pixelmatch/bin/pixelmatch" "$@"
fi
